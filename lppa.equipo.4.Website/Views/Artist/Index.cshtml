@model IEnumerable<lppa.equipo._4.BE.ArtistEE>
@{
    ViewBag.Title = "Index";
}

<h2>Artistas</h2>
@Enumerable.Count(Model)
@foreach (var item in Model)
{
    <span>@item.FirstName</span>
    <br>
    <span>@item.LastName</span>
    <br>
}
<div>
    <table>
        <tr>
            <th>
                FirstName
            </th>
            <td>
                <input id="txtFirstName" type="text" placeholder="Nombre" />
            </td>
        </tr>
        <tr>
            <th>LastName</th>
            <td> <input id="txtLastName" type="text" placeholder="Apellido" /> </td>
        </tr>
        <tr>
            <th>LifeSpan</th>
            <td> <input id="txtLifeSpan" type="text" placeholder="Años" /> </td>
        </tr>
        <tr>
            <th>Country</th>
            <td> <input id="txtCountry" type="text" placeholder="Pais" /> </td>
        </tr>
        <tr>
            <th>Description</th>
            <td> <input id="txtDescription" type="text" placeholder="Descripcion" /> </td>
        </tr>
        <tr>
            <th></th>
            <td>
                <input type="button" value="Grabar" id="btnSave" class="myButton" />
                <input type="button" value="Actualizar" id="btnUpDate" class="myButton" />
                <input type="button" value="Borrar" id="btnDelete" class="myButton" />
                <input type="button" value="Listar" id="btnToList" class="myButton" />
            </td>
        </tr>
    </table>

</div>

